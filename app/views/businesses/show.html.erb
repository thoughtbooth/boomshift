<div class="row">
  <div class="col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">My Business Profile</h3>
      </div>
      <div class="panel-body">
        <p>
          <strong>Name:</strong>
          <%= @business.name %>
        </p>

        <p>
          <strong>Contact Name:</strong>
          <%= @business.contact_name %>
        </p>

        <p>
          <strong>About:</strong>
          <%= @business.about %>
        </p>

        <p>
          <strong>Addr1:</strong>
          <%= @business.addr1 %>
        </p>

        <p>
          <strong>Addr2:</strong>
          <%= @business.addr2 %>
        </p>

        <p>
          <strong>City:</strong>
          <%= @business.city %>
        </p>

        <p>
          <strong>State:</strong>
          <%= @business.state %>
        </p>

        <p>
          <strong>Postal Code:</strong>
          <%= @business.postal_code %>
        </p>

        <p>
          <strong>Country:</strong>
          <%= @business.country %>
        </p>

        <p>
          <strong>Phone:</strong>
          <%= @business.phone %>
        </p>
      </div>
      <div class="panel-footer">
        <%= link_to edit_business_path(@business), class: "btn btn-default" do %>
        <i class="fa fa-pencil-square-o"></i> Edit
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-md-8"></div>
</div>