<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Job</th>
      <th>Description</th>
      <th>Rate/Hr</th>
      <th>Hours</th>
      <th>Total</th>
    </tr>
  </thead>
  
  <tbody>
    <% @invoicing_line_items.each do |invoicing_line_item| %>
      <tr>
        <td><%= invoicing_line_item.job_id %></td>
        <td><%= invoicing_line_item.description %></td>
        <td><%= number_to_currency invoicing_line_item.service_rate %></td>
        <td><%= invoicing_line_item.job_hours %></td>
        <td><%= number_to_currency invoicing_line_item.net_amount %></td>
      </tr>
    <% end %>
  </tbody>
</table>